df_hotspot_modis_sumsel_harian


df_hotspot_modis_sumsel_harian.describe()


# Menampilkan plot time serie
visual_time_series(df_hotspot_modis_sumsel_harian["acq_date"],
                   df_hotspot_modis_sumsel_harian["hotspot"],
                   "", "", "Hotspot Sumsel", "", "True")





# load dataset
